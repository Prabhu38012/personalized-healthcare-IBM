
GROQ_API_KEY=your_groq_api_key_here

GEMINI_API_KEY=your_actual_gemini_api_key_here

# Alternative OpenAI Configuration (if needed)
# OPENAI_API_KEY=your_openai_api_key_here

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8002

# Frontend Configuration
FRONTEND_PORT=8503

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET_KEY=your-jwt-secret-key-here

# Database (if needed in future)
DATABASE_URL=sqlite:///./healthcare.db

# Development vs Production
ENVIRONMENT=development

# CORS Settings
CORS_ORIGINS=["http://localhost:3000","http://localhost:8501","http://localhost:8503","http://localhost:8000","http://localhost:8002"]
CORS_METHODS=["GET","POST","PUT","DELETE"]
CORS_HEADERS=["*"]

# Email Notifications (if needed in future)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Healthcare Specific Configuration
RISK_THRESHOLD_HIGH=0.7
RISK_THRESHOLD_MEDIUM=0.4
DEFAULT_RISK_LEVEL=Low

# Monitoring Configuration (if needed in future)
ENABLE_METRICS=True
METRICS_PORT=9090

# Drug Advisor / NLP Configuration
# Hugging Face model for drug NER (fallback to regex if unavailable)
HF_DRUG_NER_MODEL=kamalkraj/BioBERT-NER
## Enable high-recall Hugging Face NER (loads a large model on startup). Set to 0 for fast regex extractor.
ENABLE_HF_NER=0

# IBM Watson NLU (optional)
# Set BOTH to enable enrichment in Drug Advisor. Obtain these from IBM Cloud â†’ Natural Language Understanding service.
# Example URL format includes version query: .../v1/analyze?version=2022-04-07
WATSON_API_KEY=your_watson_api_key
WATSON_URL=https://api.us-south.natural-language-understanding.watson.cloud.ibm.com/instances/your-instance-id/v1/analyze?version=2022-04-07

# openFDA base endpoint (public)
OPENFDA_BASE=https://api.fda.gov

# RxNav interaction API base (public)
RXNAV_BASE=https://rxnav.nlm.nih.gov/REST
